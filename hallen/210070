	<script>
	function initMap() {
		var halle = {lat: 49.347, lng: 7.2243};
		var map = new google.maps.Map(document.getElementById('map'), {
			zoom: 15,
			center: halle
,			mapTypeControl: false,
			scaleControl: true
		});
		var marker = new google.maps.Marker({
			position: halle,
			map: map,
			title: 'Sporthalle Wellesweiler',
			lable: 'H',
			Clickable: true
		});
		var contentString = '<h3>Sporthalle Wellesweiler</h3>
<p>D 66539 Neunkirchen</p><p>Berthold-Günther-Platz</p><a href = "https://www.google.de/maps/dir//49.347,7.2243/">Route: Sporthalle Wellesweiler</a>';
		var infowindow = new google.maps.InfoWindow({content: contentString});
		var statischinfowindow = new.google.maps.InfoWindow({content: contentString});
		marker.addListener('click', function() { infowindow.open(map, marker);});
	}
	</script>

	<script async defer  src="https://maps.googleapis.com/maps/api/js?key=AIzaSyB-EkKn6WTLH7QbuJGDyH2hNxkiru_K6Qo&callback=initMap"></script>

